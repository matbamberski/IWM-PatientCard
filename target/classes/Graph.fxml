<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<HBox maxHeight="400.0" maxWidth="600.0" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.GraphController">
    <children>
          <VBox>
              <children>
            <ChoiceBox prefWidth="150.0" id="observationChoice" />
            <Region prefHeight="50.0" prefWidth="10.0" />
                  <Button fx:id="dayButton" mnemonicParsing="false" onAction="#changeToDay" prefHeight="50.0" prefWidth="150.0" text="Day" />
                  <Button fx:id="weekButton" mnemonicParsing="false" onAction="#changeToWeek" prefHeight="50.0" prefWidth="150.0" text="Week" />
                  <Button fx:id="monthButton" mnemonicParsing="false" onAction="#changeToMonth" prefHeight="50.0" prefWidth="150.0" text="Month" />
                  <Button fx:id="yearButton" mnemonicParsing="false" onAction="#changeToYear" prefHeight="50.0" prefWidth="150.0" text="Year" />
            <Region prefHeight="200.0" prefWidth="10.0" />
                <Button fx:id="backToPatient" mnemonicParsing="false" onAction="#backToPatientLineView" prefHeight="50.0" prefWidth="150.0" text="Wstecz" />
              </children>
          </VBox>
    </children>
</HBox>
